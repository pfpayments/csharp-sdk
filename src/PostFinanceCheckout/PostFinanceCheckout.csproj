<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net6.0</TargetFrameworks>
		<SignAssembly>true</SignAssembly>
		<DelaySign>false</DelaySign>
		<AssemblyOriginatorKeyFile>..\..\AssemblyOriginatorKeyFile.snk</AssemblyOriginatorKeyFile>
    </PropertyGroup>
    <PropertyGroup>
        <AssemblyName>PostFinanceCheckout</AssemblyName>
        <AssemblyTitle>PostFinance Checkout SDK for C#</AssemblyTitle>
        <Authors>wallee AG</Authors>
        <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
        <Company>wallee AG</Company>
        <Copyright>Apache-2.0</Copyright>
        <Description>PostFinance Checkout SDK for C#</Description>
        <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
        <PackageIconUrl>https://app-wallee.com/assets/images/wallee-icon-primary.svg</PackageIconUrl>
        <PackageId>PostFinanceCheckout</PackageId>
        <PackageLicense>https://www.apache.org/licenses/LICENSE-2.0.txt</PackageLicense>
        <PackageProjectUrl>https://github.com/pfpayments/csharp-sdk</PackageProjectUrl>
        <PackageVersion>7.0.3</PackageVersion>
        <Version>7.0.3</Version>
        <PackageTags>PostFinanceCheckout;payment;sdk;Payment Integration</PackageTags>
        <OutputType>Library</OutputType>
        <Owners>customweb</Owners>
        <ReleaseVersion>7.0.3</ReleaseVersion>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/pfpayments/csharp-sdk.git</RepositoryUrl>
        <RootNamespace>PostFinanceCheckout</RootNamespace>
        <Summary>PostFinance Checkout SDK for C#</Summary>
        <SynchReleaseVersion>false</SynchReleaseVersion>
        <Title>PostFinanceCheckout</Title>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="RestSharp" Version="110.2.0"/>
        <PackageReference Include="Newtonsoft.Json" Version="13.0.3"/>
        <PackageReference Include="JsonSubTypes" Version="2.0.1"/>
        <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
        <PackageReference Include="BouncyCastle.Cryptography" Version="2.0.0" />
    </ItemGroup>

        <ItemGroup>
            <Reference Include="System"/>
            <Reference Include="System.Net.Http"/>
            <Reference Include="System.Core"/>
            <Reference Include="System.Xml.Linq"/>
            <Reference Include="System.Data.DataSetExtensions"/>
            <Reference Include="System.ComponentModel.DataAnnotations"/>
            <Reference Include="Microsoft.CSharp"/>
            <Reference Include="System.Data"/>
            <Reference Include="System.Runtime.Serialization"/>
            <Reference Include="System.Xml"/>
        </ItemGroup>

</Project>